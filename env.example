# Database Configuration
# PostgreSQL connection string (e.g., from Neon, Supabase, or local)
DATABASE_URL=postgresql://user:password@host:5432/database

# OpenAI Configuration (Backend Only - NEVER add VITE_ prefix)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_EMBED_MODEL=text-embedding-3-small
OPENAI_CHAT_MODEL=gpt-4o-mini
OPENAI_BATCH_SIZE=96

# Qdrant Configuration (Backend Only - accessed via server proxy)
# Note: These still use VITE_ prefix for compatibility but are only used server-side
VITE_QDRANT_URL=https://your-qdrant-instance.cloud.qdrant.io
VITE_QDRANT_API_KEY=your_qdrant_api_key_here
VITE_QDRANT_COLLECTION=data

# ElevenLabs Configuration (Frontend - Voice features)
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
VITE_ELEVENLABS_VOICE_ID=JBFqnCBsd6RMkjVDRZzb

# Supabase Configuration (Optional - For conversation history)
# Get from: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_SERVICE_ROLE=your_supabase_service_role_key_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
